{
  "buildCommand": "echo 'No build required for static site'",
  "outputDirectory": ".",
  "public": true,
  "env": {
    "API_BASE_URL": "@api_base_url",
    "SELLER_API_BASE": "@seller_api_base"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/profile.html",
      "dest": "/profile.html"
    },
    {
      "src": "/index.html",
      "dest": "/index.html"
    }
  ]
}
