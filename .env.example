# Frontend Environment Variables
# Copy this file to .env.local and update values for your environment

# API Base URLs
# For local development
API_BASE_URL=http://localhost:8080
PRICE_API_BASE=http://localhost:8082/api/v1
SELLER_API_BASE=http://localhost:8081/api/v1

# For production (Vercel)
# API_BASE_URL=https://your-backend-domain.com
# PRICE_API_BASE=https://your-backend-domain.com:8082/api/v1
# SELLER_API_BASE=https://your-backend-domain.com:8081/api/v1

# Test User ID (optional)
TEST_USER_ID=123456789

# Environment
NODE_ENV=development
